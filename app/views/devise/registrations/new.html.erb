<div id="login" class="bottom-three">
  <h3 class="text-center text-white pt-5">Register</h3>
  <div class="container">
    <div id="login-row" class="row justify-content-center align-items-center">
      <div id="login-column" class="col-md-8">
        <div id="login-box" class="col-md-12">
          <%= form_for(resource, as: resource_name, url: registration_path(resource_name), class:"form" , id: "login-form" ) do |f| %>
            <%= render "devise/shared/error_messages", resource: resource %>
            <h3 class="text-center text-info">Register</h3>
            <%= render partial: 'devise/registrations/commonInfo', locals: {f: f} %>
            <div>
              <%= f.submit "Submit",  class: "btn btn-info btn-md"%>
              <!-- <%= link_to "#{t 'global.cancel'}", articles_path , class: "text-info register"  %> -->
            </div>

          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
